{
  "lerna": "2.4.0",
  "packages": [
    "packages/*",
    "src/*"
  ],
  "commands": {
    "publish": {
      "ignore": [
        "src/*"
      ]
    },
    "bootstrap": {
      "ignore": [
        "packages/*"
      ]
    }
  },
  "npmClientArgs": [
    "--access public"
  ],
  "version": "1.0.0"
}
